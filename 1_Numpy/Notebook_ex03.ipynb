    "import numpy as np\n",
    "\n",
    "arr = np.arange(1, 101, 1, dtype=int)\n",
    "\n",
    "odd = arr[::2]\n",
    "even = arr[::-2]\n",
    "\n",
    "evry3 = arr\n",
    "\n",
    "evry3[1::3] = 0\n",
    "\n",
    "print(arr,\"\\n\")\n",
    "print(odd,\"\\n\")\n",
    "print(even,\"\\n\")\n",
    "print(evry3,\"\\n\")"
